//Get a three-digit number from the user,make the one's digit 2,and then print it
#include<stdio.h>
int main()
{
    int a,d1,d2,d3,Result;
    printf("Enter a number:");
    scanf("%d",&a);
    d1=(a/100)%10;
    d2=(a/10)%10;
    d3=2;
    Result=(d1*100)+(d2*10)+d3;
    printf("%d",Result);
    return 0;
}
