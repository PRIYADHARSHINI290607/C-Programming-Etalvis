//Get a three-digit number  from the user and subtract 5 from that number if the one's digit number and 100's digit number are odd,then print the result .Dont use"if
#include<stdio.h>
int main()
{
    int a,d1,d2,Result;
    printf("Enter a number:");
    scanf("%d",&a);
    d1=(a/100);
    d2=(a%10);
    Result=a-5*(d1%2)*(d2%2);
    printf("%d",Result);
    return 0;
}
