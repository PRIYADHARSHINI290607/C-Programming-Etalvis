//Get a three-digit number from the user and check if the digit 1 and digit 0  are the same.If yes,then print"success",otherwise print"Failure"
#include<stdio.h>
int main()
{
    int a,d0,d1;
    printf("Enter a number:");
    scanf("%d",&a);
    d0=(a%10);
    d1=(a/10)%10;
    if(d0==d1)
    printf("Success");
    else
    printf("Failure");
    return 0;
}
