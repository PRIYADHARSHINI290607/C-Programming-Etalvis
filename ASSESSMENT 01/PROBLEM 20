//Get a number  from the user and subtract 5 from that number if the number's ten's position digit is odd,then print the result .Dont use"if
#include<stdio.h>
int main()
{
    int a,d1,Result;
    printf("Enter a number:");
    scanf("%d",&a);
    d1=(a/10)%10;
    Result=a-5*(d1%2);
    printf("%d",Result);
    return 0;
}
