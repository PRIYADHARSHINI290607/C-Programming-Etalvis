// write a loop program to print the sum of two-digit odd numbers,whose ten's digit is 7 
#include<stdio.h>
int main()
{
    int a=10,sum=0,d0,d1;
    loop:if(a<100)
    {
        d0=(a%10);
        d1=(a/10);
         if(a%2!=0)
         {
             if(d1==7)
             {
            sum=sum+a;
             }
          }
         a++;
         goto loop;
    }
    else
    printf("%d",sum);
    return 0;
}
